# Node container environment variables
NODE_ENV=development
CYPRESS_USER_PLATFORM_URL=https://your-user-platform-url.com
CYPRESS_CREDENTIALS_PATH=/path/to/credentials.json

# Python container environment variables
PYTHONUNBUFFERED=1
PYTHONPATH=/app
CUSTOMER_DATA_PATH=/path/to/customer/data
POLYLINE_PATH=/path/to/polyline

# Prisma client library environment variables
PRISMA_ENDPOINT=https://your-prisma-endpoint.com
PRISMA_SECRET=your-prisma-secret
